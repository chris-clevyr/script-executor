{
  "name": "Script Executor",
  "version": "0.0.1",
  "description": "Executed scripts on a page",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/",
    "menus",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{0beb8936-6e0d-4284-a1e8-567059cfb3b3}",
      "strict_min_version": "42.0"
    }
  },
  "manifest_version": 2
}
